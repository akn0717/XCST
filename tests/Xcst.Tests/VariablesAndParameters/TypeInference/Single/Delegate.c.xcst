<?xml version="1.0" encoding="utf-8" ?>
<c:module version='1.0' language='C#' xmlns:c='http://maxtoroq.github.io/XCST'>

   <c:template name='c:initial-template'>
      <c:variable name='a'>
         <c:delegate/>
      </c:variable>
      <c:variable name='b'>
         <c:delegate as='string'>b</c:delegate>
      </c:variable>
      <c:void value='Assert.AreEqual(typeof(Xcst.XcstDelegate&lt;object>), CompileType(a))'/>
      <c:void value='Assert.AreEqual(typeof(Xcst.XcstDelegate&lt;string>), CompileType(b))'/>
   </c:template>

</c:module>