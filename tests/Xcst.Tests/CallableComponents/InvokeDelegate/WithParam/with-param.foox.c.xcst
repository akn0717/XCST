<?xml version="1.0" encoding="utf-8" ?>
<c:module version='1.0' language='C#' xmlns:c='http://maxtoroq.github.io/XCST'>

   <c:template name='foo'>
      <c:variable name='bar'>
         <c:delegate>
            <c:param name='a'/>
         </c:delegate>
      </c:variable>
      <c:invoke-delegate delegate='bar'>
         <c:with-param name='a' value='null' foo:foo='foo' xmlns:foo='urn:foo'/>
      </c:invoke-delegate>
   </c:template>

</c:module>