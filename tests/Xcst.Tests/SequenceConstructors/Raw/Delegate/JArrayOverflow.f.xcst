<?xml version="1.0" encoding="utf-8" ?>
<c:module version='1.0' language='C#' xmlns:c='http://maxtoroq.github.io/XCST'>

   <c:import-namespace ns='Newtonsoft.Json.Linq'/>

   <c:template name='c:initial-template'>
      <c:variable name='del'>
         <c:delegate as='int'>
            <c:object value='1'/>
            <c:object value='2'/>
            <c:void value='Assert.Fail()'/>
         </c:delegate>
      </c:variable>
      <c:variable name='a' as='JArray'>
         <c:array>
            <c:invoke-delegate delegate='del'/>
         </c:array>
      </c:variable>
   </c:template>

</c:module>