<?xml version="1.0" encoding="utf-8" ?>
<c:module version='1.0' language='C#' xmlns:c='http://maxtoroq.github.io/XCST'>

   <c:template name='c:initial-template'>
      <c:variable name='map' as='dynamic'>
         <c:map>
            <c:map-entry key='"foo"'>
               <c:object value='default(object)'/>
            </c:map-entry>
         </c:map>
