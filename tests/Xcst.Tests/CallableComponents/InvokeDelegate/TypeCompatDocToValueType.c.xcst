<?xml version="1.0" encoding="utf-8" ?>
<c:module version='1.0' language='C#' xmlns:c='http://maxtoroq.github.io/XCST'>

   <c:template name='c:initial-template'>
      <c:variable name='foo'>
         <c:delegate as='int'>
            <c:object value='5'/>
         </c:delegate>
      </c:variable>
      <foo>
         <c:invoke-delegate delegate='foo'/>
      </foo>
   </c:template>

   <c:template name='expected'>
      <foo>5</foo>
   </c:template>

</c:module>